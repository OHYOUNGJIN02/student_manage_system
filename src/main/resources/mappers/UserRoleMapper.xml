<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.korit.student_manage_system.mapper.UserRoleMapper">

    <insert id="addUserRole" useGeneratedKeys="true" keyProperty="userRoleId">
        INSERT INTO user_role_tb
        (user_id, role_id, create_dt, update_dt)
        VALUES
        (#{userId}, #{roleId}, NOW(), NULL)
    </insert>

</mapper>
