<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.korit.student_manage_system.mapper.ClassMapper">

    <insert id="addClass" useGeneratedKeys="true" keyProperty="classId">
        insert into user_tb
        values (0, #{clsName}, #{clsDscpt)
    </insert>

    <delete id="deleceClasss">
        select
            *
        where
            cls_id = #{clsId};
    </delete>

</mapper>

